@article{nguyen2021,
  title = {A Long-Term Study of {{AAV}} Gene Therapy in Dogs with Hemophilia {{A}} Identifies Clonal Expansions of Transduced Liver Cells},
  author = {Nguyen, Giang N. and Everett, John K. and Kafle, Samita and Roche, Aoife M. and Raymond, Hayley E. and Leiby, Jacob and Wood, Christian and Assenmacher, Charles-Antoine and Merricks, Elizabeth P. and Long, C. Tyler and Kazazian, Haig H. and Nichols, Timothy C. and Bushman, Frederic D. and Sabatino, Denise E.},
